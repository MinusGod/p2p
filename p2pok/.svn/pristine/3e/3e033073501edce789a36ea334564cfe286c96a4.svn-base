		<h2 class="y_bdh2">竞拍记录</h2>
		             	<table class="y_bdtab2">
							<tbody>
								<tr>
									<th>序号</th>
									<th>竞拍人</th>
									<th>竞拍出价</th>
									<th>竞拍时间</th>
									<th>竞拍状态</th>
								</tr>
						#{list items:page.page, as:'records'}
								<tr>
									<td>${records_index}</td>
									<td>${records?.name?.asterisk()}</td>
									<td>${records?.offer_price?.format("###,##0.00")}元</td>
									<td>${records?.time?.format('yyyy-MM-dd HH:mm:ss')}</td>
									<td>
									#{if records?.status==0}
									正常
									#{/if}
									#{if records?.status==1}
									成交
									#{/if}
									#{if records?.status==2}
									待接受
									#{/if}
									#{if records?.status==3}
									待确认
									#{/if}
									#{if records?.status==-1}
									失败
									#{/if}
									
									</td>
								</tr>
						#{/list}		
							</tbody>
						</table>
					<div class="y_page">
		    <!-- 分页器 -->
			#{page funMethod:"showDebtAuctionRecords",currPage:page.currPage,pageSize:page.pageSize,totalCount:page.totalCount,theme:2,style:2 /}
	                       </div>
	
	
	
	
