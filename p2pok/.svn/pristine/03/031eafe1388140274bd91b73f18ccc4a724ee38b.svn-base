	<h2 class="y_bdh2">投标记录</h2>
			<table class="y_bdtab2">
				<tr>
					<td >序号</td>
					<td >投标人</td>
					<td >投标金额</td>
					<td >投标时间</td>
					<td >竞拍状态</td>
				</tr>
                #{list items:pageBean?.page , as: 'records'}
                <tr>
                    <td>${records_index} </td>
                    <td>${records?.name?.asterisk()} </td>
                    <td>${records?.invest_amount}元 </td>
                    <td>${records?.time?.format('yyyy-MM-dd HH:mm:ss')} </td>
                    <td>
                        #{if records?.transfer_status == 0}
                        正常
                        #{/if}
                        #{if records?.transfer_status == -1}
                        已转让出
                        #{/if}
                        #{if records?.transfer_status == 1}
                        转让中
                        #{/if}
                    </td>
                </tr>
                #{/list}
			</table>	
			<!-- 分页器 -->
			<div class="y_page">
			#{page funMethod:"showBidRecords",currPage:pageBean.currPage,pageSize:pageBean.pageSize,totalCount:pageBean.totalCount,theme:2,style:3 /}
			</div>
	
	
