package models;

import java.util.Date;

import javax.persistence.Entity;

import play.db.jpa.Model;

/**
 * 直客推广名单表
 *
 * @author hys
 * @createDate  2015年9月17日 下午5:34:00
 *
 */
@Entity
public class t_spread_users extends Model{
	
	/** 创建时间 */
	public Date time;
	
	/** 管理员id */
	public long supervisor_id;
	
	/** 被推广用户id */
	public long user_id;
	
	/** 直客推广码 */
	public String spread_code;
	
	/** 被推广用户账号 */
	public String user_name;
	
	/** 被推广用户注册时间 */
	public Date user_register_time;
	
	/** 被推广人联系方式 */
	public String mobile;
	
	/** 被推广用户充值金额 */
	public double recharge_amount;
	
	/** 被推广用户投资金额 */
	public double invest_amount;
	
	/** 投资优惠百分比 */
	public int discount_rate;
	
}
